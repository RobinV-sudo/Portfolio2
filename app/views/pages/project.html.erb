  <div class="row">
    <div class="col-md-12">
      <div class="project_spacetrip">
        <h2 class="project_spacetrip_title">Project Spacetrip</h2>
          <div id="slideshow_project">
            <img src="spacetrip_1.jpg" alt= "Photo 1" class= "active_project">
            <img src="spacetrip_2.jpg" alt= "Photo 2">
            <img src="spacetrip_3.jpg" alt= "Photo 3">
            <img src="spacetrip_4.jpg" alt= "Photo 4">
            <img src="spacetrip_5.jpg" alt= "Photo 5">
            <!-- local server pictures not found -->
          </div>
          <div class="btn_container_project">
            <button class="btn_project" onclick="showSlideP(1)">Photo 1</button>
            <button class="btn_project" onclick="showSlideP(2)">Photo 2</button>
            <button class="btn_project" onclick="showSlideP(3)">Photo 3</button>
            <button class="btn_project" onclick="showSlideP(4)">Photo 4</button>
            <button class="btn_project" onclick="showSlideP(5)">Photo 5</button>
          </div>
          <script>
            let currentSlide = 1;

            function showSlideP(slideNumber) {
              const slides = document.querySelectorAll('#slideshow_project img');
              const buttons = document.querySelectorAll('.btn_container_project');

              slides[currentSlide - 1].classList.remove('active_project');
              slides[slideNumber - 1].classList.add('active_project');

              buttons[currentSlide - 1].classList.remove('active_project');
              buttons[slideNumber - 1].classList.add('active_project');

              currentSlide = slideNumber;
            }
          </script>
      </div>
    </div>
  </div>
